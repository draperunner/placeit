{
  "name": "placeit",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "emulators": "firebase emulators:start --import=./emulators-export",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" \"./functions/src/**/*.ts\"",
    "deploy": "npm run build && firebase deploy",
    "format": "prettier . --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "blobs": "^2.2.0",
    "firebase": "^12.3.0",
    "maplibre-gl": "^5.8.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-map-gl": "^8.0.4",
    "react-router-dom": "^7.9.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
