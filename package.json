{
  "name": "placeit",
  "version": "0.2.0",
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "ts": "tsc --noEmit",
    "emulators": "firebase emulators:start --import=./emulators-export",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" \"./functions/src/**/*.ts\"",
    "deploy": "firebase deploy",
    "format": "prettier . --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@turf/turf": "^7.3.1",
    "blobs": "^2.2.0",
    "firebase": "^12.6.0",
    "maplibre-gl": "^5.14.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-map-gl": "^8.1.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6"
  }
}
